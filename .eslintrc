{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
   "jsx-a11y/media-has-caption": [
    1,
    {
      "audio": [
        "Audio"
      ],
      "video": [
        "Video"
      ],
      "track": [
        "Track"
      ]
    }
  ]
  }
}